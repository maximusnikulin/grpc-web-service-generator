{
  "name": "grpc-web-service-generator",
  "version": "0.1.8",
  "description": "gRPC-Web Service Generator",
  "author": "quarrant",
  "license": "MIT",
  "keywords": [
    "grpc",
    "grpc-web"
  ],
  "homepage": "https://github.com/quarrant/grpc-web-service-generator#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quarrant/grpc-web-service-generator.git"
  },
  "bugs": {
    "url": "https://github.com/quarrant/grpc-web-service-generator/issues"
  },
  "bin": {
    "grpc-web-service-generator": "lib/cli.js"
  },
  "scripts": {
    "build": "rm -rf lib && tsc",
    "prepublishOnly": "yarn build",
    "generate": "ts-node ./src/cli.ts --proto-file=./proto/test.proto --output-dir=./proto",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": ">=10.1.0"
  },
  "dependencies": {
    "protobufjs": "^6.11.2",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.8",
    "@types/node": "^16.4.10",
    "@types/yargs": "^17.0.2",
    "grpc-web": "^1.2.1",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "uglify-js": "^3.14.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
